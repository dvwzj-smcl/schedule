{
  "name": "semi-schedule",
  "version": "1.0.0",
  "description": "Schedule Project",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* resources/assets/react tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"resources/assets/react/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root resources/assets/react --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"resources/assets/react/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root resources/assets/react --report lcovonly _mocha -- --require ./tools/testSetup.js \"resources/assets/react/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "prod": "gulp --production",
    "dev": "gulp watch"
  },
  "author": "dvwzj-smcl",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "es6-promise": "3.2.1",
    "flexboxgrid": "6.3.1",
    "formsy-material-ui": "0.5.0",
    "formsy-react": "0.18.0",
    "fullcalendar": "2.9.1",
    "gemini-scrollbar": "1.4.3",
    "isomorphic-fetch": "2.2.1",
    "jquery-ui": "1.12.0",
    "jsonwebtoken": "7.0.1",
    "material-ui": "0.15.4",
    "object-assign": "4.1.0",
    "react": "15.3.0",
    "react-color": "*",
    "react-dom": "15.3.0",
    "react-flexbox-grid": "0.10.2",
    "react-gemini-scrollbar": "2.1.5",
    "react-paginate": "*",
    "react-redux": "4.4.5",
